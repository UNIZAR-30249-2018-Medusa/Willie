SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;

--
-- Roles
--

CREATE ROLE postgres;
ALTER ROLE postgres WITH SUPERUSER INHERIT CREATEROLE CREATEDB LOGIN REPLICATION BYPASSRLS PASSWORD 'md53175bce1d3201d16594cebf9d7eb3f9d';
CREATE ROLE willieuser;
ALTER ROLE willieuser WITH SUPERUSER INHERIT NOCREATEROLE NOCREATEDB LOGIN NOREPLICATION NOBYPASSRLS PASSWORD 'md5c7f796748949c07efc568effcba407ca';






--
-- Database creation
--

CREATE DATABASE gisdb WITH TEMPLATE = template0 OWNER = postgres;
ALTER DATABASE gisdb SET search_path TO public, postgis, contrib, topology;
REVOKE CONNECT,TEMPORARY ON DATABASE template1 FROM PUBLIC;
GRANT CONNECT ON DATABASE template1 TO PUBLIC;


\connect gisdb

SET default_transaction_read_only = off;

